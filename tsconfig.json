{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noUncheckedIndexedAccess": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "strict": true,
    "paths": {
      "@api/*": ["src/api/endpoint/*"],
      "@schema/*": ["src/api/schemas/*"],
      "@expects/*": ["src/expects/*"],
      "@fixtures/*": ["src/utils/pageFixtures/*"],
      "@helpers/*": ["src/api/helpers/*"]
    }
  }
}
